}
#source-items, #dest-items{
    text-align:center;
}

.pin-left{
    float:left;
}

.pin-right:hover{
    color:red;
    cursor: pointer;
}

#target{
    display:none;
}

.red-notice{
    font-size:13px;
    color:#e74c3c;
    font-style:italic;
}

.slick-slide{
    width: 600px;
}

#header {
    position:fixed;
    height: 100%;
    width:100%;
    margin-left:-30px !important;
    margin-top:-30px;
}


.headerBg {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.index-expl{
    color:#FFF;
    text-align:center;
}

.image-container{
    width:1200px;
    margin:0 auto;

}
.slick-arrow{
    background:rgba(0,0,0,0.5) !important;
    height:50px !important;
    width:50px !important;
    z-index: 999 !important;
}

.steps{
    text-align: center;
    color:#000;
    font-size: 2em;
    position:absolute;
    width:1200px;
}

.index-expl img{
    width:1200px;
}
.container .main{

}

#main-desc{
    color:#fff;
    text-align: center;
}

.main-login{
    text-align:center;
    font-size: 3em;
    margin-top: 260px;
    background-color:#FFF;
}

.view-flight-btn{
    margin-top:1em;
}

#results{
    margin-top:20px;
}

.show {
  display: block !important;
}
.hidden {
  display: none !important;
  visibility: hidden !important;
}
.invisible {
  visibility: hidden;
}

#toggle-hide{
    width:100%;
    float:right;
    height:20px;
    border-bottom:1px solid #D0D0D0;
    text-align:left;
}

#setting-text{
    float:left;
    height:21px;
    line-height:10px;
    letter-spacing:none;
    word-spacing:none;
    
}

.flight-item{
    width:300px;
    height:300px;
    float:left;
    margin:10px;
    padding:5px;
    text-align:center;
}

.shadow{
    -webkit-box-shadow: -2px 2px 9px -1px rgba(0,0,0,0.75);
    -moz-box-shadow: -2px 2px 9px -1px rgba(0,0,0,0.75);
     box-shadow: -2px 2px 9px -1px rgba(0,0,0,0.75);
}

.destination-item{
    text-align:center;
}

.price-links a{
    font-size:5em;
    -webkit-transform:rotate(90deg);
    -moz-transform:rotate(90deg);
    -o-transform:rotate(90deg);
    transform:rotate(90deg);
    display:block;
    text-align:center;
    color:#0B62A4;
}

.price-links a:hover{}

.glyphicon .glyphicon-plane{
    font-size:3em;
    text-align: center;
    color:#0B62A4;
}

#minus-dest{
    cursor:pointer;
}

@media only screen and (min-device-width : 375px) and (max-device-width : 750px) { 
    .crop img {
        width:150%;
        height: 100%;
        margin-left:-15px;
    }

    .crop {
        width: 97%;
        height: 150%;
        overflow: hidden;
        border:solid 1px #DDDDDD;
        margin-top:15px;
        box-shadow: -1px -1px 3px grey;
    }
    .col-md-3{
        width:100% !important;
    }
    
}

/* Medium devices (desktops, 992px and up) */
@media screen and (min-device-width : 992px) and (max-device-width : 1200px) {  
    .crop img {
        width:150%;
        height: 100%;
        margin-left:-15px;
    }
    .col-md-3{
        width:50% !important;
    }

    .crop {
        width: 97%;
        height: 150%;
        overflow: hidden;
        border:solid 1px #DDDDDD;
        margin-top:15px;
        box-shadow: -1px -1px 3px grey;
    }
}



@media screen and (min-device-width : 650px) and (max-device-width : 990px) {  
    .crop img {
        width:150%;
        height: 100%;
        margin-left:-15px;
    }
    .col-md-3{
        width:100% !important;
    }

    .crop {
        width: 97%;
        height: 150%;
        overflow: hidden;
        border:solid 1px #DDDDDD;
        margin-top:15px;
        box-shadow: -1px -1px 3px grey;
    }
}
/* Large devices (large desktops, 1200px and up) */
